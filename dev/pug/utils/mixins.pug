mixin icon(name,mod,width,height,alt)
	if(mod)
		svg(class="svg-sprite-icon icon-" + name + ' ' + mod width=width height=height alt=alt)
			use(xlink:href="static/images/svg/symbol/sprite.svg#" + name)
	else
		svg(class="svg-sprite-icon icon-" + name)
			use(xlink:href="static/images/svg/symbol/sprite.svg#" + name)
